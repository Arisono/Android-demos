            android:process=":remote" >
            <intent-filter>
                <action android:name="com.baidu.location.service_v2.2" >
                </action>
            </intent-filter>
        </service>        
        <meta-data
            android:name="com.baidu.lbsapi.API_KEY"
            android:value="X99MVEVA8LKed3gtu6OKvlWRmtRBL5yx" />  <!--http://lbsyun.baidu.com/apiconsole/key-->

        <!-- meta-data需要写在application中 -->
        <activity
            android:name="com.baidu.location.demo.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.baidu.location.demo.LocationActivity" >
        </activity>
        <activity android:name="com.baidu.location.demo.LocationOption"></activity>
